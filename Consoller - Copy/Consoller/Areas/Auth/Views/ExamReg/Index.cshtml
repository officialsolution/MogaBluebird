@model IEnumerable<Consoller.Areas.Auth.Models.tblfill>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Auth/Views/Shared/Layout.cshtml";
}

<h2><b>Exam Registration</b></h2>

<p>
    @Html.ActionLink("Create New", "Create", "", new { @class = "btn btn-primary" })
</p>
<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.date)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.passport)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.dob)
                </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.doe)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.choice1)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.choice2)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.choice3)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.module)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.v1)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.v2)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.v3)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.mode)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.instname)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.status)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.fid)
                    </th>*@
                @*<th>
                        Username
                    </th>
                    <th>
                        Password
                    </th>*@
                <th>Action</th>
            </tr>
        </thead>
        @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Convert.ToDateTime(@item.date).ToString("dd-MM-yyyy")
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.passport)
                </td>
                @*<td>
                        @Convert.ToDateTime(@item.dob).ToString("dd-MM-yyyy")
                    </td>*@
                <td>
                    @Convert.ToDateTime(@item.doe).ToString("dd-MM-yyyy")
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.choice1)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.choice2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.choice3)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.module)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.v1)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.v2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.v3)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.mode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.instname)
                </td>
                <td>

                    @Html.DisplayFor(modelItem => item.status)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.fid)
                    </td>*@
                @*<td>
                        @Html.DisplayFor(modelItem => item.uname)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.pass)
                    </td>*@
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "label label-warning" }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }, new { @class = "label label-primary" }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "label label-danger" })
                </td>
            </tr>
        }

    </table>
</div>