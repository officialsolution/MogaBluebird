@model IEnumerable<Consoller.Areas.Auth.Models.SubmitFile>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubmitDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubmitBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileDoc)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubmitDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubmitBy)
        </td>
        <td>
           <a href="~/Uploadfile/@item.FileDoc" target="_blank" class="badge badge-primary">Download File</a>
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Sid }) |
            @Html.ActionLink("Details", "Details", new { id=item.Sid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Sid })
        </td>
    </tr>
}

</table>
